<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stage_open" model="project.task.type">
            <field name="sequence">1</field>
            <field name="name">Open</field>
        </record>
        <record id="stage_invoice" model="project.task.type">
            <field name="sequence">10</field>
            <field name="name">Invoicing</field>
        </record>
        <record id="stage_closed" model="project.task.type">
            <field name="sequence">20</field>
            <field name="name">Closed</field>
            <field name="fold" eval="True"/>
        </record>
        <record id="stage_canceled" model="project.task.type">
            <field name="sequence">30</field>
            <field name="name">Cancelled</field>
            <field name="fold" eval="True"/>
        </record>

        <record id="project_freight_forwarding" model="project.project">
            <field name="name">Freight Forwarding</field>
            <field name="type_ids" eval="[(6, 0, [ref('stage_open'), ref('stage_invoice'), ref('stage_closed'), ref('stage_canceled')])]"/>
        </record>
    </data>
